#!/bin/bash
eye "$@"  http://josd.github.io/eye/reasoning/gps/gps-plugin.n3  http://josd.github.io/eye/reasoning/map/agent1-map.n3 --query http://josd.github.io/eye/reasoning/map/agent1-query.n3 > agent1-proof.n3
eye "$@"  http://josd.github.io/eye/reasoning/gps/gps-plugin.n3  http://josd.github.io/eye/reasoning/map/agent2-map.n3 --twinkle http://josd.github.io/eye/reasoning/map/agent1-proof.n3 --query http://josd.github.io/eye/reasoning/map/agent2-query.n3 > agent2-proof.n3
