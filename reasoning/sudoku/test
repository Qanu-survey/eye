#!/bin/bash
eye "$@" --nope --prolog http://josd.github.io/eye/reasoning/sudoku/sudoku_program.pl --query http://josd.github.io/eye/reasoning/sudoku/sudoku_query.n3 > sudoku_answer.n3
eye "$@" --prolog http://josd.github.io/eye/reasoning/sudoku/sudoku_program.pl --query http://josd.github.io/eye/reasoning/sudoku/sudoku_query.n3 > sudoku_proof.n3
