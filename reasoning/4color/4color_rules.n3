PREFIX list: <http://www.w3.org/2000/10/swap/list#>
PREFIX : <http://josd.github.io/eye/reasoning/4color#>

{  
    ?COUNTRY :neighbours ?NEIGHBOURS.
    :map :coloring ?COLOR.
    false <= {
        ?COUNTRY :color ?ANY.
    }.
    false <= {
        ?NEIGHBOURS list:member ?NEIGHBOUR.
        ?NEIGHBOUR :color ?COLOR.
    }.
} => {
    ?COUNTRY :color ?COLOR.
}.
