#!/bin/bash
eye "$@" --nope --plugin http://josd.github.io/eye/reasoning/fft/fft_program.eyepl --query http://josd.github.io/eye/reasoning/fft/fft_query.n3 > fft_answer.n3
eye "$@" --plugin http://josd.github.io/eye/reasoning/fft/fft_program.eyepl --query http://josd.github.io/eye/reasoning/fft/fft_query.n3 > fft_proof.n3
